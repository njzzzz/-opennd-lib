{
  "name": "@opennd/lib",
  "type": "module",
  "version": "0.0.3",
  "author": "njzzzz",
  "repository": {
    "url": "https://github.com/njzzzz/-opennd-lib"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./lib/*": [
      "./lib/*",
      "./lib/*/index.ts",
      "./lib/*/index.tsx"
    ]
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs",
    "index.ts",
    "lib"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "tsx watch ./playground.ts",
    "api-docs": "api-extractor run --local && npx api-documenter markdown -i ./temp -o ./docs/docs/pages/Node",
    "changelog": "conventional-changelog -i ./docs/docs/pages/Changelog/index.md -s -r 0",
    "publish": "pnpm publish --access=public --publish-branch main",
    "bumpp": "bumpp -r",
    "release": "run-s build api-docs changelog"
  },
  "dependencies": {
    "exceljs": ">= 4.x",
    "express": ">= 4.x",
    "mongoose": ">= 5.x",
    "short-uuid": "^5.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@microsoft/api-documenter": "^7.25.3",
    "@microsoft/api-extractor": "^7.47.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/express": "^4.17.21",
    "@types/node": "20",
    "bumpp": "^9.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1"
  }
}
